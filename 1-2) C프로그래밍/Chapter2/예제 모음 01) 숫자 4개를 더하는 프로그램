#include<stdio.h>

int main() 
{
	int n1, n2, n3, n4;

	printf("첫 번째 계산할 값을 입력하세요 ==> ");
	scanf_s("%d", &n1);

	printf("두 번째 계산할 값을 입력하세요 ==> ");
	scanf_s("%d", &n2);

	printf("세 번째 계산할 값을 입력하세요 ==> ");
	scanf_s("%d", &n3);

	printf("네 번째 계산할 값을 입력하세요 ==> ");
	scanf_s("%d", &n4);

	printf("%d + %d + %d + %d = %d\n", n1, n2, n3, n4, (n1+n2+n3+n4));
}
